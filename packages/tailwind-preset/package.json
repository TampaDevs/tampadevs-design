{
  "name": "@tampadevs/tailwind-preset",
  "version": "0.1.0",
  "description": "Tailwind CSS v4 preset for Tampa Devs design system",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./theme.css": "./dist/theme.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && cp src/theme.css dist/theme.css",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "tailwindcss": "^4.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
